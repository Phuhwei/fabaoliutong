{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "allowJs": true,
    "noImplicitAny": true,
    "removeComments": true,
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "lib": [
      "es2017",
      "dom",
    ]
  },
  "exclude": [
    "api",
    "mytest*",
    "test",
    "build"
  ],
}